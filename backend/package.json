    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.9.0",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "tsc-alias": "^1.8.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
